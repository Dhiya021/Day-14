int main() 
{
    int a, b, c;
    float interest, amount, discount, final_amount;
    scanf("%d", &a);
    scanf("%d", &b);
    scanf("%d", &c);
    interest = (a * b * c) / 100.0;
    amount = a + interest;
    discount = (2 * interest) / 100.0;
    final_amount = amount - discount;
    printf("%.2f\n", interest);
    printf("%.2f\n", amount);
    printf("%.2f\n", discount);
    printf("%.2f\n", final_amount);

    return 0;
}